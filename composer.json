{
    "name": "certificationy/certificationy-cli",
    "type": "library",
    "description": "A CLI tool to train on Symfony certification",
    "keywords": ["symfony", "certification", "cli", "test", "exam"],
    "homepage": "http://github.com/certificationy/certificationy-cli",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Composieux",
            "email": "vincent.composieux@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/console": ">=2.5",
        "symfony/finder": ">=2.5",
        "symfony/yaml": ">=2.5",
        "certificationy/certificationy": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": { "Certificationy\\Cli\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
